{
    "packages": [
        {
            "name": "Laravel Breeze",
            "repository": "laravel/breeze",
            "post_install": [
                "php artisan breeze:install",
                "php artisan migrate"
            ]
        },
        {
            "name": "Spatie Laravel Permission",
            "repository": "spatie/laravel-permission",
            "post_install": [
                "php artisan vendor:publish --provider='Spatie\Permission\PermissionServiceProvider'",
                "php artisan migrate"
            ]
        },
        {
            "name": "Livewire",
            "repository": "livewire/livewire",
            "version": "^3.0",
            "post_install": [
                "php artisan vendor:publish --tag=config"
            ]
        },
        {
            "name": "Laravel Debugbar",
            "repository": "barryvdh/laravel-debugbar",
            "version": "^3.0",
            "post_install": [
                "php artisan vendor:publish --provider='Barryvdh\Debugbar\ServiceProvider'"
            ]
        },
        {
            "name": "Wire-elements Modal",
            "repository": "wire-elements/modal",
            "post_install": [
                "php artisan vendor:publish --tag=config"
            ]
        },
        {
            "name": "Eloquent Blameable",
            "repository": "richan-fongdasen/eloquent-blameable",
            "post_install": [
                "php artisan vendor:publish --tag=config"
            ]
        },
        {
            "name": "Laravel Medialibrary",
            "repository": "spatie/laravel-medialibrary",
            "post_install": [
                "php artisan vendor:publish --provider='Spatie\MediaLibrary\MediaLibraryServiceProvider'",
                "php artisan migrate"
            ]
        },
        {
            "name": "Livewire Dropzone",
            "repository": "dasundev/livewire-dropzone",
            "post_install": [
                "php artisan vendor:publish --tag=config"
            ]
        }
    ]
}
